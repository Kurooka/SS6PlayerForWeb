{
  "name": "ss6player-pixi",
  "version": "1.1.0",
  "description": "SS6Player for pixi",
  "keywords": [
    "SpriteStudio",
    "SS6Player",
    "2D",
    "Animation",
    "Sprite",
    "Skeletal",
    "pixi"
  ],
  "main": "dist/ss6player-pixi.umd.js",
  "module": "dist/ss6player-pixi.es5.js",
  "typings": "dist/types/ss6player-pixi.d.ts",
  "bugs": {
    "url": "https://github.com/SpriteStudio/SS6PlayerForWeb/issues"
  },
  "homepage": "https://github.com/SpriteStudio/SS6PlayerForWeb#readme",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Web Technology Corp.",
    "url": "https://www.webtech.co.jp/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SpriteStudio/SS6PlayerForWeb.git"
  },
  "license": "SEE LICENSE",
  "scripts": {
    "start": "npm run watch",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "npm run clean",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "docs": "typedoc --out ../../docs/ss6player_pixi_api --target es6 --theme minimal --mode file src",
    "watch": "rollup -c rollup.config.ts -w",
    "dist": "npm run docs && npm run build",
    "view": "concurrently \"http-server -c-1 ../../\" \"opener http://localhost:8080/packages/ss6player-pixi/Player/index.html\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "flatbuffers": "^1.11.0",
    "pixi.js": "4.8.5",
    "ssfblib": "^1.0.0"
  },
  "devDependencies": {
    "@types/flatbuffers": "^1.9.1",
    "@types/pixi.js": "4.8.8",
    "concurrently": "^4.1.2",
    "http-server": "^0.11.1",
    "lodash.camelcase": "^4.3.0",
    "opener": "^1.5.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.2",
    "tslint": "^5.19.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.2"
  }
}
